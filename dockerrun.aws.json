{
  "AWSEBDockerrunVersion":2,
  "containerDefinitions":[
    {
      "name": "client",
      "image": "s0261105/multi-client",
      "hostname": "client",
      "essential": false
    },
    {
      "name": "server",
      "image": "s0261105/multi-server",
      "hostname": "api",
      "essential": false
    },
    {
      "name": "worker",
      "image": "s0261105/multi-worker",
      "hostname": "worker",
      "essential": false
    },
    {
      "name":"nginx",
      "image": "s0261105/multi-nginx",
      "essential": true,
      "portMappings":[
        {
          "hostPort":80,
          "containerPort":80
        }
      ],

      "links":[ "client" , "server"]
    }

  ]
}
